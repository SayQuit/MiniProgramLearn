.nav{
  width: 95%;
  height: 200rpx;
  /* border: 1px solid; */
  margin: 0 auto;
  display: flex;
}
swiper-item navigator{
  width: 100%;
  height: 100%;
}
swiper-item image{
  width: 100%;
  height: 100%;
}
.nav navigator{
  flex: 1;
  /* border: 1px solid; */
  position: relative;
  height: 100%;
  /* border: 0; */
}

.navtitle{
  position: absolute;
  color: #999;
  left: 50%;
  transform: translateX(-50%);
  top: 62.5%;
}
.navimg{
  position: absolute;
  color: #999;
  left: 50%;
  transform: translateX(-50%);
  top: 15%;
  width: 70%;
  height: 70%;
}



.floor{
  height: 500rpx;
  width: 100%;
}
.floor view{
  flex: 1;
}
.floor navigator{
  width: 100%;
  height: 100%;
}
.floor image{
  width: 100%;
  height: 100%;
}

.floor_title{
  width: 100%;
  height: 100rpx;
}